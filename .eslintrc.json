{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "serviceworker": true
  },
  "parser": "babel-eslint",
  "extends": ["next", "prettier"],
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "require": true,
    "app": true,
    "module": true
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "no-unused-vars": ["warn"],
    "indent": ["warn", 2],
    "semi": ["error", "always"],
    "one-var-declaration-per-line": ["error", "initializations"],
    "no-console": 0,
    "no-debugger": 0
  }
}
